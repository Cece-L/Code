package com.briup.ce18;

import java.util.HashSet;
import java.util.Set;

public class Test {
    public static void main(String[] args) {
		new Test().test1();
	}
    public void 	test1(){
    	//set的创建
	   Set set = new HashSet();
	   //set的添加
	   set.add(1);
	   set.add("Tom");
	   set.add(1.1);
	   set.add(new Student(1001, "Tom", 11));
	   set.clear();
	   System.out.println(set.size());
   }
}

package com.briup.ce19;

import java.sql.Connection;
import java.sql.DriverManager;

public class Basic {
	public static void main(String[] args) {
		new Basic().test1();
	}
	//插入
	public void test1(){
		try {
			//1.注册驱动
			Class.forName("com.mysql.jdbc.Driver");
			//2.
			Connection conn = DriverManager.getConnection("jdbc:mysql://127.0.0.1:3360/ce19", "root", "root");
			System.out.println(conn);
		} catch (Exception e) {
			e.printStackTrace();
		}
	}
}


